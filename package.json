{
  "name": "tokopedia-lite",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "autoprefixer": "6.5.3",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.7",
    "babel-plugin-syntax-trailing-function-commas": "6.13.0",
    "babel-plugin-transform-class-properties": "6.19.0",
    "babel-plugin-transform-object-rest-spread": "6.19.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2016": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-runtime": "6.18.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "cross-spawn": "5.0.1",
    "css-loader": "0.26.0",
    "eslint": "3.10.2",
    "eslint-loader": "1.6.1",
    "eslint-plugin-flowtype": "2.25.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.1",
    "eslint-plugin-react": "6.7.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "fs-extra": "1.0.0",
    "gh-pages": "^0.12.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.24.1",
    "http-server": "^0.9.0",
    "json-loader": "0.5.4",
    "opn": "4.0.2",
    "postcss-loader": "1.1.1",
    "promise": "7.1.1",
    "redbox-react": "^1.3.3",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "sw-precache": "^4.0.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "1.16.2",
    "webpack-hot-middleware": "^2.13.2",
    "whatwg-fetch": "2.0.1"
  },
  "dependencies": {
    "babel-preset-stage-0": "^6.16.0",
    "better-npm-run": "0.0.13",
    "ip": "^1.1.4",
    "node-sass": "^3.13.0",
    "normalize.css": "^5.0.0",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-slick": "^0.14.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.2"
  },
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build config server src tests",
    "lint:css": "stylelint \"./src/**/*.scss\" --syntax \"scss\"",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "flow:init": "flow init",
    "flow:check": "flow check"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run test && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "eslintConfig": {
    "extends": "./config/eslint.js"
  },
  "homepage": "./"
}
