    "flow:init": "flow init",
    "flow:check": "flow check"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run lint:css && npm run test && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:staging": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    },
    "deploy:beta": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "beta",
